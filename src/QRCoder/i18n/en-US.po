msgid "生成二维码，用法：/qr <内容> [尺寸(不指定则为自适应)]"
msgstr "Generate QR code, using: /qr <content> [Size (if not specified, it is adaptive)]"

#: ../../QRCoder.cs:159
msgid "用法：/qr <内容> [尺寸(不指定则为自适应)]"
msgstr "Usage: /qr <content> [Size (if not specified, it is adaptive)]"

#: ../../QRCoder.cs:302
msgid "用法：/qrconf <键> <值>"
msgstr "Usage: /qrconf <key> <value>"

#: ../../QRCoder.cs:113
msgid "用法：/qrpos <tl|bl|tr|br> - tl=左上角，bl=左下角，tr=右上角，br=右下角"
msgstr "Usage: /qrpos <tl|bl|tr|br> - tl=upper left corner, bl=lower left corner, tr=upper right corner, br=lower right corner"

#: ../../QRCoder.cs:35
msgid "设置二维码位置，用法：/qrpos <tl|bl|tr|br>，tl=左上角，bl=左下角，tr=右上角，br=右下角"
